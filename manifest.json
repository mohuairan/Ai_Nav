{
  "manifest_version": 3,
  "name": "AI Quick Nav",
  "version": "11.5",
  "description": "为 Gemini, ChatGPT 和 DeepSeek 提供侧边栏目录导航",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": [
        "*://gemini.google.com/*",
        "*://chatgpt.com/*",
        "*://chat.openai.com/*",
        "*://chat.deepseek.com/*"
      ],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
